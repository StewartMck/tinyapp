<%- include('partials/_header');%>
    <title>TinyApp - New TinyURL</title>
  </head>
  <body>
    <%- include('partials/_navbar') %>
    <main style="margin: 1em;">
      <h2>Create TinyURL</h2>
      <form class="form-inline" action="/urls" method="POST">
        <div class="form-group mb-2">
          <label for="longURL">Enter a URL:</label>
          <input class="form-control" type="text" name="longURL" placeholder="http://" style="width: 300px; margin: 1em"></input>
          <button type="submit" class="btn btn-primary btn-lg">Submit</button>
        </div>
      </form>
    </main>
    <%- include('partials/_footer');%>